{
  "name": "ku4es-ui-testing",
  "version": "1.0.0",
  "description": "kodmunki Utilities for ECMAScript UI Testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodmunki/ku4es-ui-testing"
  },
  "license": "MIT",
  "engines": {
    "node": "8.*",
    "npm": "5.*"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install && npm run link",
    "test": "mocha -R spec --compilers js:babel-register --recursive ./test",
    "test-file": "mocha -R spec --compilers js:babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline",
    "link": "sh ./link.sh"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "canvas": "^1.6.7",
    "jsdom": "^11.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "6.*",
    "babel-preset-es2015": "6.*",
    "babel-preset-stage-3": "6.*",
    "babel-register": "^6.26.0",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "^5.0.2",
    "eslint": "^4.7.2",
    "eslint-plugin-import": "2.*",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "shx": "0.*"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist"
}
