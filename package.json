{
  "name": "ku4es-ui-testing",
  "version": "1.5.0",
  "description": "kodmunki Utilities for ECMAScript UI Testing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodmunki/ku4es-ui-testing.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install",
    "test": "mocha -R spec --compilers js:babel-register --recursive ./test",
    "test-file": "mocha -R spec --compilers js:babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "docs": "npm run jsdocs && npm run mddocs",
    "docs-read": "open -a 'Google Chrome' ./jsdoc/index.html",
    "jsdocs": "rm -rf jsdoc ; jsdoc -c ./jsdoc.json",
    "mddocs": "rm README.API.md ; jsdoc2md --files 'src/**/*.js' > README.API.md",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline",
    "prepare": "npm run build"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "canvas": "^1.6.13",
    "jsdom": "11.11.0",
    "ku4es-kernel": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc-to-markdown": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^11.9.0",
    "shx": "^0.2.2"
  },
  "files": [
    "dist"
  ],
  "main": "dist",
  "bugs": {
    "url": "https://github.com/kodmunki/ku4es-ui-testing/issues"
  },
  "homepage": "https://github.com/kodmunki/ku4es-ui-testing#readme",
  "author": "kodmunki"
}
