{
  "name": "@kodmunki/ku4es-ui-testing",
  "version": "1.0.1",
  "description": "kodmunki Utilities for ECMAScript UI Testing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodmunki/ku4es-ui-testing.git"
  },
  "license": "MIT",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "scripts": {
    "reload": "rm -rf ./node_modules && rm -f ./package-lock.json && npm install",
    "test": "mocha -R spec --compilers js:babel-register --recursive ./test",
    "test-file": "mocha -R spec --compilers js:babel-register",
    "test-cov": "cross-env NODE_ENV=test nyc npm test",
    "test-cov-d": "open -a 'Google Chrome' ./coverage/lcov-report/index.html",
    "lint": "eslint './src/**/*.js' './test/**/*.js'",
    "lint-fix": "eslint --fix './src/**/*.js' './test/**/*.js'",
    "check": "npm run lint && npm run test-cov",
    "check-d": "npm run test-cov-d",
    "build": "shx mkdir -p dist && babel src -d dist --source-maps inline"
  },
  "dependencies": {
    "@kodmunki/ku4es-kernel": "^1.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "canvas": "^1.6.7",
    "jsdom": "^11.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-stage-0": "6.x",
    "babel-register": "6.x",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "4.x",
    "nyc": "^11.4.0",
    "shx": "0.x"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist",
  "bugs": {
    "url": "https://github.com/kodmunki/ku4es-ui-testing/issues"
  },
  "homepage": "https://github.com/kodmunki/ku4es-ui-testing#readme",
  "author": "kodmunki"
}
